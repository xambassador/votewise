{
  "name": "votewise-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts":{
    "web:start": "lerna exec --scope @votewise/web -- yarn start",
    "web:dev": "lerna exec --scope @votewise/web -- yarn dev",
    "web:build": "lerna exec --scope @votewise/web -- yarn build",
    "server:start": "lerna exec --scope @votewise/server -- yarn start",
    "server:dev": "lerna exec --scope @votewise/server -- yarn dev",
    "server:build": "lerna exec --scope @votewise/server -- yarn build",
    "clean": "lerna clean --yes && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "lerna": "^6.5.1",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5"
  },
  "packageManager": "yarn@1.22.19"
}
