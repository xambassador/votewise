{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "display": "Node 16",
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES6",
      "noImplicitAny": true,
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "lib": ["ES6"],
      "declaration": true
    },
    "exclude": ["node_modules"]
  }
